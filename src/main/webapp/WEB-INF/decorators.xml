<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/decorators">
    <excludes>
        <pattern>/css/*</pattern>
        <pattern>/resources/css/*</pattern>
        <pattern>/img/*</pattern>
        <pattern>/resources/img/*</pattern>
        <pattern>/js/*</pattern>
        <pattern>/resources/js/*</pattern>
        <pattern>*.json</pattern>
        <pattern>*.xml</pattern>
        <pattern>/rest/*</pattern>
        <pattern>/ajax/*</pattern>

    </excludes>

    <decorator name="main-layout" page="decorator.jsp">
        <pattern>*</pattern>
    </decorator>

    <!--	<decorator name="layoutModal" page="layoutModal.jsp">
          		<pattern>/resources1/*</pattern>
   	</decorator>-->



</decorators>